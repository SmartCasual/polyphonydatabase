<h1>Voicings</h1>

<h2>Add voicing</h2>
<div class="topform">
  <%= form_for([:admin, Voicing.new]) do |f| %>
    <%= f.text_field :text %>
    <button>Create</button>
  <% end %>
</div>

<h2>Edit existing voicings</h2>

<ul class="mainlist">
  <% Voicing.order(:text).each do |voicing| %>
    <li><%= link_to voicing.text, edit_admin_voicing_path(voicing) %></li>
  <% end %>
</ul>
